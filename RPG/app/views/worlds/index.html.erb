<h1>Welcome!</h1>

<h3> Hi <%= @user.name %>! You have <%= @user.cores.count %> <%= link_to "core(s)", inventory_path %>.</h3>

<h1>Select a world to go to!</h1>
<% @worlds.each do |world| %>
  <% unless world.name == "Raccoon World"%>
    <%= link_to(world.name, world) %><br>
  <% end %>
<% end %>
